{
  "name": "ts-utils",
  "version": "1.0.0",
  "description": "Vue3 工具库演示",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite --config vite.lib.config.ts",
    "dev:web": "vite",
    "dev:test": "vitest",
    "build": "vue-tsc && vite build --config vite.lib.config.ts",
    "build:web": "vite build",
    "test": "vitest run",
    "lint": "eslint src --ext .ts,.tsx,.vue",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "keywords": ["vue3", "utils", "typescript"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "ant-design-vue": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0"
  }
} 